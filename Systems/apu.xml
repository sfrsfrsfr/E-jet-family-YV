<PropertyList>
    <filter>
        <name>APU starter power source</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <property>/controls/electric/external-power-connected</property>
            </condition>
            <value>2</value>
        </input>
        <input>
            <condition>
                <property>/controls/electric/battery-switch[1]</property>
            </condition>
            <value>1</value>
        </input>
        <input>
            <value>0</value>
        </input>
        <output>/engines/apu/starter-power-source</output>
    </filter>

    <filter>
        <name>APU starter power</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/starter-power-source</property>
                    <value>2</value>
                </equals>
            </condition>
            <property>/systems/electrical/sources/dc-gpu/volts</property>
        </input>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/starter-power-source</property>
                    <value>1</value>
                </equals>
            </condition>
            <property>/systems/electrical/sources/battery[1]/volts</property>
        </input>
        <input>
            <value>0</value>
        </input>
        <output>/engines/apu/starter-power</output>
    </filter>

    <filter>
        <name>APU RPM target</name>
        <type>gain</type>
        <gain>100</gain>
        <input>
            <!-- insufficient power for starter -->
            <condition>
                <less-than>
                    <property>/engines/apu/starter-power</property>
                    <value>18</value>
                </less-than>
            </condition>
            <value>0</value>
        </input>
        <input>
            <!-- starter switch in "OFF" position -->
            <condition>
                <not>
                    <property>/controls/apu/starter</property>
                </not>
            </condition>
            <value>0</value>
        </input>
        <input>
            <!-- starter switch in "START" position, but no fuel flow -->
            <condition>
                <and>
                    <less-than>
                        <property>engines/apu/feed</property>
                        <value>0</value>
                    </less-than>
                    <equals>
                        <property>/controls/apu/starter</property>
                        <value>2</value>
                    </equals>
                </and>
            </condition>
            <value>0.25</value>
        </input>
        <input>
            <!-- starter switch in "ON" position, but no fuel flow -->
            <condition>
                <and>
                    <less-than>
                        <property>engines/apu/feed</property>
                        <value>0</value>
                    </less-than>
                    <equals>
                        <property>/controls/apu/starter</property>
                        <value>1</value>
                    </equals>
                </and>
            </condition>
            <value>0</value>
        </input>
        <input>
            <!-- starter switch in "START" or "ON" position, and everything OK -->
            <value>1</value>
        </input>
        <output>/engines/apu/rpm-target</output>
    </filter>

    <filter>
        <name>APU Feed</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <and>
                    <property>/fdm/jsbsim/propulsion/tank[1]/priority</property>
                    <greater-than>
                        <property>/fdm/jsbsim/propulsion/tank[1]/contents-lbs</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <value>1</value>
        </input>
        <input>
            <condition>
                <and>
                    <property>/fdm/jsbsim/propulsion/tank[0]/priority</property>
                    <greater-than>
                        <property>/fdm/jsbsim/propulsion/tank[0]/contents-lbs</property>
                        <value>0</value>
                    </greater-than>
                    <!-- may feed on left tank when crossfeed valve is open,
                         set to "LOW 2", and either the left wing ejector pump
                         or the left wing AC pump are running -->
                    <equals>
                        <property>/systems/fuel/crossfeed-valve</property>
                        <value>1</value>
                    </equals>
                    <or>
                        <property>/systems/fuel/fuel-pump[0]/running</property>
                        <property>/engines/engine[0]/running</property>
                    </or>
                </and>
            </condition>
            <value>0</value>
        </input>
        <input>
            <condition>
                <and>
                    <property>/fdm/jsbsim/propulsion/tank[2]/priority</property>
                    <greater-than>
                        <property>/fdm/jsbsim/propulsion/tank[2]/contents-lbs</property>
                        <value>2</value>
                    </greater-than>
                    <!-- can only use tank 3 if AC fuel pump 3 is running -->
                    <property>/systems/fuel/fuel-pump[2]/running</property>
                </and>
            </condition>
            <value>2</value>
        </input>
        <input>
            <value>-1</value>
        </input>
        <output>/engines/apu/feed</output>
    </filter>

    <filter>
        <name>APU RPM</name>
        <type>noise-spike</type>
        <max-rate-of-change>
            <condition>
                <less-than>
                    <property>/engines/apu/rpm-target</property>
                    <property>/engines/apu/rpm</property>
                </less-than>
            </condition>
            <value>9.0</value>
        </max-rate-of-change>
        <max-rate-of-change>3.0</max-rate-of-change>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/feed</property>
                    <value>-1</value>
                </equals>
            </condition>
            <expression>
                <min>
                    <property>/engines/apu/rpm-target</property>
                    <value>25</value>
                </min>
            </expression>
        </input>
        <input>
            <property>/engines/apu/rpm-target</property>
        </input>
        <output>/engines/apu/rpm</output>
    </filter>

    <filter>
        <name>APU ready</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <greater-than>
                    <property>/engines/apu/rpm</property>
                    <value>99</value>
                </greater-than>
            </condition>
            <value>1</value>
        </input>
        <input>
            <value>0</value>
        </input>
        <output>/engines/apu/running</output>
    </filter>

    <filter>
        <name>APU switch flipback</name>
        <type>gain</type>
        <gain>1</gain>
        <enable>
            <condition>
                <and>
                    <property>/engines/apu/running</property>
                    <equals>
                        <property>/controls/apu/starter</property>
                        <value>2</value>
                    </equals>
                </and>
            </condition>
        </enable>
        <input>
            <value>1</value>
        </input>
        <output>/controls/apu/starter</output>
    </filter>
    <filter>
        <name>APU startup inhibit</name>
        <type>gain</type>
        <gain>1</gain>
        <enable>
            <condition>
                <property>/engines/apu/shutting-down</property>
            </condition>
        </enable>
        <input>
            <value>0</value>
        </input>
        <output>/controls/apu/starter</output>
    </filter>

    <flipflop>
        <name>APU shutting down on</name>
        <type>RS</type>
        <S>
            <and>
                <greater-than>
                    <property>engines/apu/temp-c</property>
                    <value>100</value>
                </greater-than>
                <less-than>
                    <property>engines/apu/rpm-target</property>
                    <value>50</value>
                </less-than>
            </and>
        </S>
        <R>
            <less-than>
                <property>engines/apu/temp-c</property>
                <value>100</value>
            </less-than>
        </R>
        <output>engines/apu/shutting-down</output>
    </flipflop>

    <filter>
        <name>APU fuel valve</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <!-- shutting down: no fuel used -->
            <condition>
                <less-than>
                    <property>/engines/apu/rpm-target</property>
                    <value>1</value>
                </less-than>
            </condition>
            <value>1</value>
        </input>
        <input>
            <!-- fuel injection starts at 25% RPM -->
            <condition>
                <less-than>
                    <property>/engines/apu/rpm</property>
                    <value>25</value>
                </less-than>
            </condition>
            <value>1</value>
        </input>
        <input>
            <value>0</value>
        </input>
        <output>/engines/apu/cutoff</output>
    </filter>
    <filter>
        <name>APU fuel flow</name>
        <type>gain</type>
        <gain>-250</gain>
        <input>
            <condition>
                <property>/engines/apu/cutoff</property>
            </condition>
            <value>0</value>
        </input>
        <input>
            <expression>
                <div>
                    <property>/engines/apu/rpm</property>
                    <value>100</value>
                </div>
            </expression>
        </input>
        <output>/engines/apu/fuel-flow-rate-pph</output>
    </filter>

    <filter>
        <name>APU fuel flow tank 1</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/feed</property>
                    <value>0</value>
                </equals>
            </condition>
            <property>/engines/apu/fuel-flow-rate-pph</property>
        </input>
        <output>/systems/fuel/external-flow[0]/apu</output>
    </filter>
    <filter>
        <name>APU fuel flow tank 2</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/feed</property>
                    <value>1</value>
                </equals>
            </condition>
            <property>/engines/apu/fuel-flow-rate-pph</property>
        </input>
        <output>/systems/fuel/external-flow[1]/apu</output>
    </filter>
    <filter>
        <name>APU fuel flow tank 3</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <equals>
                    <property>/engines/apu/feed</property>
                    <value>2</value>
                </equals>
            </condition>
            <property>/engines/apu/fuel-flow-rate-pph</property>
        </input>
        <output>/systems/fuel/external-flow[2]/apu</output>
    </filter>

    <filter>
        <name>APU heat production</name>
        <type>gain</type>
        <gain>0.75</gain>
        <input>
            <condition>
                <and>
                    <greater-than>
                        <property>engines/apu/rpm</property>
                        <value>25</value>
                    </greater-than>
                    <greater-than>
                        <property>engines/apu/feed</property>
                        <value>-1</value>
                    </greater-than>
                </and>
            </condition>
            <property>engines/apu/rpm</property>
        </input>
        <input>
            <!-- no fuel flow: no heat produced -->
            <value>0</value>
        </input>
        <output>engines/apu/heat-in</output>
    </filter>
    <filter>
        <name>APU temp diff</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <difference>
                    <property>engines/apu/temp-c</property>
                    <property>environment/temperature-degc</property>
                </difference>
            </expression>
        </input>
        <output>engines/apu/temp-diff-c</output>
    </filter>
    <filter>
        <name>APU heat out</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <product>
                    <property>engines/apu/temp-diff-c</property>
                    <sum>
                        <property>engines/apu/rpm</property>
                        <value>25</value>
                    </sum>
                    <value>0.0014</value>
                </product>
            </expression>
        </input>
        <output>engines/apu/heat-out</output>
    </filter>
    <filter>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs>0.1</update-interval-secs>
        <debug type="bool">false</debug>
        <input>
            <expression>
                <sum>
                    <product>
                        <difference>
                            <property>engines/apu/heat-in</property>
                            <property>engines/apu/heat-out</property>
                        </difference>
                        <value>0.1</value>
                    </product>
                    <property>engines/apu/temp-c</property>
                </sum>
            </expression>
        </input>
        <output>engines/apu/temp-c</output>
    </filter>
</PropertyList>
